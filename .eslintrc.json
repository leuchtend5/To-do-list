{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "max-len": ["error", { "code": 100 }],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "import/extensions": ["error", "never"],
    "no-param-reassign": "off"
  }
}
